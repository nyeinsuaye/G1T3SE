<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>BootstrapServlet</servlet-name>
        <servlet-class>com.app.controller.service.BootstrapServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DropBidServlet</servlet-name>
        <servlet-class>com.app.controller.DropBidServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchCourseServlet</servlet-name>
        <servlet-class>com.app.controller.SearchCourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditBidServlet</servlet-name>
        <servlet-class>com.app.controller.EditBidServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AuthenticateServlet</servlet-name>
        <servlet-class>com.app.controller.AuthenticateServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBidServlet</servlet-name>
        <servlet-class>com.app.controller.AddBidServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DropSectionServlet</servlet-name>
        <servlet-class>com.app.controller.DropSectionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DumpTableServlet</servlet-name>
        <servlet-class>com.app.controller.service.DumpTableServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DumpUserServlet</servlet-name>
        <servlet-class>com.app.controller.service.DumpUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DumpBidServlet</servlet-name>
        <servlet-class>com.app.controller.service.DumpBidServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RoundServlet</servlet-name>
        <servlet-class>com.app.controller.service.RoundServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DumpSectionServlet</servlet-name>
        <servlet-class>com.app.controller.service.DumpSectionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchCourse</servlet-name>
        <servlet-class>com.app.controller.SearchCourseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchSectionServlet</servlet-name>
        <servlet-class>com.app.controller.SearchSectionServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>BootstrapServlet</servlet-name>
        <url-pattern>/admin/bootstrap</url-pattern>
        <url-pattern>/blabblabblab/bootstrap</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchCourseServlet</servlet-name>
        <url-pattern>/search-course-id.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DropBidServlet</servlet-name>
        <url-pattern>/delete-bid.do</url-pattern>
        <url-pattern>/blabblabblab/delete-bid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditBidServlet</servlet-name>
        <url-pattern>/edit-bid.do</url-pattern>
        <url-pattern>/blabblabblab/update-bid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AuthenticateServlet</servlet-name>
        <url-pattern>/authenticate.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBidServlet</servlet-name>
        <url-pattern>/add-bid.do</url-pattern>
        <url-pattern>/blabblabblab/add-bid</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DropSectionServlet</servlet-name>
        <url-pattern>/drop-section.do</url-pattern>
        <url-pattern>/blabblabblab/drop-section</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DumpTableServlet</servlet-name>
        <url-pattern>/blabblabblab/dump</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DumpUserServlet</servlet-name>
        <url-pattern>/blabblabblab/user-dump</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DumpBidServlet</servlet-name>
        <url-pattern>/blabblabblab/bid-dump</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RoundServlet</servlet-name>
        <url-pattern>/admin/start</url-pattern>
        <url-pattern>/admin/stop</url-pattern>
        <url-pattern>/blabblabblab/start</url-pattern>
        <url-pattern>/blabblabblab/stop</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DumpSectionServlet</servlet-name>
        <url-pattern>/blabblabblab/section-dump</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchCourse</servlet-name>
        <url-pattern>/search.html</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchSectionServlet</servlet-name>
        <url-pattern>/SearchSectionServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
